{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.formatOnSave": false,
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "files.exclude": {
    "**/node_modules": true
  },
  "code-runner.executorMapByGlob": {
    "pom.xml": "cd $dir && mvn clean package",
    "*.test.ts": "cd $workspaceRoot/packages/sbg-utility && npm test -- $fileNameWithoutExt",
    "*.spec.ts": "cd $workspaceRoot/packages/sbg-utility && npm test -- $fileNameWithoutExt"
  },
  "code-runner.showRunIconInEditorTitleMenu": false,
  "code-runner.showRunCommandInExplorerContextMenu": false,
  "code-runner.respectShebang": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.importModuleSpecifier": "relative",
  "search.exclude": {
    "**/dist/**": true,
    "**/tmp/**": true
  },
  "files.associations": {
    "*.njk": "njk",
    "*.ejs": "html"
  },
  "emmet.includeLanguages": {
    "ejs": "html",
    "njk": "html",
    "nunjucks": "html"
  },
  "emmet.syntaxProfiles": {
    "njk": "html",
  },
  "eslint.packageManager": "yarn",
  "npm.packageManager": "yarn",
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": "file:///d%3A/Repositories/static-blog-generator/.github/workflows/test-beta.yml",
    "https://json.schemastore.org/dependabot-2.0.json": "file:///d%3A/Repositories/static-blog-generator/.github/dependabot.yml"
  }
}
