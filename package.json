  "scripts": {
    "dev": "nodemon",
    "build": "npm-run-all -s build:**",
    "build:src": "ts-node build.ts",
    "refresh": "npm update hexo-post-parser safelinkify google-news-sitemap persistent-cache",
    "test": "npm-run-all -s test:**",
    "test:git": "cd tests && npm i https://github.com/dimaslanjaka/static-blog-generator/raw/master/release/development.tgz && node index.js",
    "test:mocha": "cross-env TS_NODE_PROJECT='./tsconfig.mocha.json' mocha -r ts-node/register 'src/**/*.spec.ts'",
    "test:js": "cd tests && npm i file:../ && node index.js",
    "test:ts": "cd tests && npm i file:../ && node -r ts-node/register index.ts",
    "php": "php-server",
    "eslint": "npm-run-all -s eslint:**",
    "eslint:fix": "ts-node src/eslint.ts",
    "depcheck": "depcheck > dump-unused.log",
    "page": "echo 'install custom page libraries' && npm-run-all -s  page:**",
    "page:install": "cd docs && npm install --production && fse remove .git .gitignore",
    "page:install2": "cd docs/page && npm install --production && fse remove .git .gitignore",
    "format": "run-p format:**",
    "format:themes-assets": "prettier --write ./themes/**/*.{ts,css,js,scss,sass,less}",
    "format:themes-layouts": "prettier --write --parser html -w ./themes/**/*.ejs",
    "update": "sh bin/install-repo",
    "clean-install": "npm install --ignore-engines --ignore-platform",
    "rebuild": "npm rebuild --ignore-engines --ignore-platform",
    "update-version": "npm version $(git describe)",
    "preinstall": "node scripts/preinstall.js",
    "postpack": "node scripts/postpack.js",
    "postinstall": "node scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/static-blog-generator.git"
  },
  "keywords": [
    "static",
    "blog",
    "generator",
    "hexo",
    "static-blog-generator"
