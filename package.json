{
  "name": "static-blog-generator",
  "version": "3.0.0",
  "description": "Pre-processing all source posts before rendering from hexo. Useful for low-end devices to prevent using large number of hexo plugins.",
  "main": "packages/sbg-main/dist/index.js",
  "types": "packages/sbg-main/dist/index.d.ts",
  "bin": {
    "sbg-cli": "packages/sbg-main/dist/cli.js"
  },
  "files": [
    "packages",
    "packages/sbg-*/**/*",
    "packages/sbg-*/package.json",
    "LICENSE",
    "*.json",
    "!**/node_modules/**",
    "!packages/**/node_modules/**",
    "!**/tmp/**",
    "!packages/**/tmp/**",
    "!test",
    "!**/test/**",
    "!**/*.tgz"
  ],
  "scripts": {
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:ncu": "npx npm-check-updates -u -x chalk",
    "update": "run-s update:**",
    "postupdate": "node postinstall.js"
  },
  "author": {
    "name": "Dimas Lanjaka",
    "email": "dimaslanjaka@gmail.com",
    "url": "https://webmanajemen.com"
  },
  "license": "MIT",
  "dependencies": {
    "git-command-helper": "https://gitpkg.now.sh/dimaslanjaka/static-blog-generator/packages/git-command-helper?master",
    "google-news-sitemap": "https://gitpkg.now.sh/dimaslanjaka/static-blog-generator/packages/google-news-sitemap?master",
    "hexo-post-parser": "https://gitpkg.now.sh/dimaslanjaka/static-blog-generator/packages/hexo-post-parser?master",
    "safelinkify": "https://gitpkg.now.sh/dimaslanjaka/static-blog-generator/packages/safelinkify?master",
    "sbg-api": "https://gitpkg.now.sh/dimaslanjaka/static-blog-generator/packages/sbg-api?master",
    "sbg-main": "https://gitpkg.now.sh/dimaslanjaka/static-blog-generator/packages/sbg-main?master",
    "sbg-server": "https://gitpkg.now.sh/dimaslanjaka/static-blog-generator/packages/sbg-server?master",
    "sbg-utility": "https://gitpkg.now.sh/dimaslanjaka/static-blog-generator/packages/sbg-utility?master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/static-blog-generator.git"
  },
  "keywords": [
    "gulp",
    "blog",
    "static-site-generator",
    "hexo",
    "static-site",
    "blog-nodejs",
    "hexo-admin",
    "hexo-seo",
    "hexo-gui",
    "gulp-blog"
  ],
  "bugs": {
    "url": "https://github.com/dimaslanjaka/static-blog-generator/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/static-blog-generator#readme"
}
