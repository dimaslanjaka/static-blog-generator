{
  "name": "static-blog-generator",
  "version": "2.0.7-beta-f0b7a152f5",
  "description": "Static Blog Generator",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "sbg": "./bin/sbg.js"
  },
  "files": [
    "src",
    "gulpfile.ts",
    "themes",
    "tsconfig.json",
    "README.md",
    "_config.yml",
    "source/guide",
    "source/package.json",
    "dist",
    "packages",
    "bin"
  ],
  "scripts": {
    "dev": "nodemon",
    "build": "npm-run-all -s  build:**",
    "build:src": "ts-node build.ts && yarn refresh",
    "build:hexo-post-parser": "cd packages/hexo-post-parser && yarn install --check-files",
    "build:safelinkify": "cd packages/safelink && yarn install --check-files",
    "refresh": "yarn upgrade hexo-post-parser safelinkify google-news-sitemap persistent-cache",
    "test": "npm-run-all -s test:**",
    "test:js": "cd tests && npm i file:../ && node index.js",
    "test:ts": "cd tests && npm i file:../ && node -r ts-node/register index.ts",
    "test:git": "cd tests && npm i dimaslanjaka/static-blog-generator#dev && node index.js",
    "test:mocha": "env TS_NODE_PROJECT='./tsconfig.mocha.json' mocha -r ts-node/register 'src/**/*.spec*.ts'",
    "php": "php -S localhost:4002 -t docs",
    "eslint": "npm-run-all -s  eslint:**",
    "eslint:fix": "ts-node src/eslint.ts",
    "depcheck": "depcheck > dump-unused.log",
    "page": "echo 'install custom page libraries' && npm-run-all -s  page:**",
    "page:install": "cd docs && npm install --production && fse remove .git .gitignore",
    "page:install2": "cd docs/page && npm install --production && fse remove .git .gitignore",
    "format": "run-p format:**",
    "format:themes-assets": "prettier --write ./themes/**/*.{ts,css,js,scss,sass,less}",
    "format:themes-layouts": "prettier --write --parser html -w ./themes/**/*.ejs",
    "ci": "npm-run-all -s  ci:**",
    "ci:submodule": "git submodule foreach yarn install --check-files",
    "ci:self": "yarn install --check-cache --check-files --ignore-engines --ignore-platform",
    "rebuild": "yarn rebuild --ignore-engines --ignore-platform"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/static-blog-generator.git"
  },
  "keywords": [
    "static",
    "blog",
    "generator",
    "hexo",
    "static-blog-generator"
  ],
  "author": {
    "email": "dimaslanjaka@gmail.com",
