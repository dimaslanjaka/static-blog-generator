{
  "name": "static-blog-generator-hexo",
  "version": "1.0.0-158-g02c28cc",
  "private": true,
  "scripts": {
    "start": "npm run server",
    "build": "sbg copy --nocache && hexo generate",
    "postbuild": "gulp copy && gulp safelink",
    "copy": "sbg copy --nocache",
    "clean": "sbg clean && hexo clean",
    "deploy": "npm run build && gulp deploy",
    "server": "hexo server -p 4000",
    "server-php": "php -S localhost:4000 -t public",
    "postinstall": "node bin/update-cache.js",
    "update": "npx npm-check-updates -u -x hexo,chalk && npm run refresh",
    "refresh": "npm update hexo-adsense hexo-browsersync hexo-front-matter hexo-generator-redirect static-blog-generator safelinkify hexo-post-parser"
  },
