{
  "name": "static-blog-generator",
  "version": "3.0.0",
  "description": "Pre-processing all source posts before rendering from hexo. Useful for low-end devices to prevent using large number of hexo plugins.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.js",
    "readme.md",
    "LICENSE"
  ],
  "scripts": {
    "update": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.github/workflows/build-release.yml > .github/workflows/build-release.yml && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js && npx npm-check-updates -u -x chalk,hexo && npm install --cache tmp/npm",
    "postinstall": "node postinstall.js",
    "build": "tsc -p tsconfig.build.json && gulp copy",
    "postbuild": "node packer.js",
    "docs": "gulp docs",
    "dev": "tsc -p tsconfig.build.json -w",
    "lint": "npx eslint --fix src/**/*.{ts,js,json}",
    "test": "jest --env node --coverage --maxConcurrency=1 --maxWorkers=50%"
  },
  "keywords": [
    "static-blog-generator",
    "static",
    "blog",
    "generator"
  ],
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com> (https://webmanajemen.com)",
  "license": "ISC",
  "peerDependencies": {
    "hexo": "^6.3.0",
    "slugify": "^1.6.5"
  },
  "dependencies": {
    "@prettier/plugin-xml": "^2.2.0",
    "ansi-colors": "^4.1.3",
    "bluebird": "^3.7.2",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/tarball/typescript",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.1.0",
    "git-command-helper": "https://github.com/dimaslanjaka/git-command-helper/raw/master/release/git-command-helper.tgz",
    "gulp": "^4.0.2",
    "gulp-dom": "https://github.com/dimaslanjaka/static-blog-generator-hexo/raw/master/packages/gulp-dom/release/gulp-dom.tgz",
    "hexo-post-parser": "https://github.com/dimaslanjaka/hexo-post-parser/raw/master/release/hexo-post-parser.tgz",
    "hexo-util": "^3.0.1",
    "micromatch": "^4.0.5",
    "moment-timezone": "^0.5.40",
    "nunjucks": "^3.2.3",
    "persistent-cache": "https://github.com/dimaslanjaka/persistent-cache/raw/improve2/release/persistent-cache.tgz",
    "safelinkify": "https://github.com/dimaslanjaka/safelink/raw/master/release/safelinkify.tgz",
    "sitemap-crawler": "https://github.com/dimaslanjaka/static-blog-generator-hexo/raw/master/packages/sitemap-crawler/release/sitemap-crawler.tgz",
    "through2": "^4.0.2",
    "upath": "^2.0.1",
    "yaml": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@types/bluebird": "^3.5.38",
    "@types/gulp": "^4.0.10",
    "@types/hexo": "https://github.com/dimaslanjaka/nodejs-package-types/raw/hexo/release/types-hexo.tgz",
    "@types/hexo-util": "https://github.com/dimaslanjaka/nodejs-package-types/raw/main/hexo-util/release/types-hexo-util.tgz",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "@types/through2": "https://github.com/dimaslanjaka/nodejs-package-types/tarball/through2",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "babel-jest": "^29.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodejs-package-types": "https://github.com/dimaslanjaka/nodejs-package-types/raw/main/release/nodejs-package-types.tgz",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.8",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.23",
    "typedoc-plugin-missing-exports": "^1.0.0"
  }
}
