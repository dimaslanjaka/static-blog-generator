{
  "compilerOptions": {
    "declarationMap": true,
    "composite": true,
    "incremental": true,
    "declaration": true,
    "pretty": true,
    "lib": ["DOM", "es2015.iterable", "es2015.generator", "es5"],
    "typeRoots": ["node_modules/@types"],
    "target": "es5",
    "moduleResolution": "node",
    "outDir": "./dist",
    "strictNullChecks": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false,
    "newLine": "lf",
    "types": [],
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["project.js"],
  "plugins": [
    {
      "transform": "typescript-transform-paths"
    },
    {
      "transform": "typescript-transform-paths",
      "afterDeclarations": true
    }
  ]
}
