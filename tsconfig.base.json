{
  "compilerOptions": {
    "incremental": true,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "Bundler",
    "declaration": true,
    "sourceMap": true,
    "strict": false,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    //"composite": true,
    "outDir": "tmp/dist"
  },
  "plugins": [
    {
      "transform": "typescript-transform-paths"
    },
    {
      "transform": "typescript-transform-paths",
      "afterDeclarations": true
    }
  ]
}
