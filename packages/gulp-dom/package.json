    "chai": "4.3.6",
    "gulp": "4.0.2",
    "gulp-concat": "^2.6.1",
    "jshint": "2.13.5",
    "mocha": "10.1.0",
    "request": "2.88.0",
    "typescript": "^4.8.4",
    "vinyl": "^3.0.0",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0"
