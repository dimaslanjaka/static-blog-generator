{
  "name": "gulp-dom",
  "version": "1.0.1",
  "description": "Gulp plugin for generic DOM manipulation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "src",
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:trygve-lie/gulp-dom.git"
  },
  "bugs": {
