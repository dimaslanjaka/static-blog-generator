<!doctype html>
<html class="h-full bg-gray-100">
  <head>
    <meta charset="utf-8">
    <title>
      {% block title %}Express Nunjucks TailwindCSS{% endblock %}
    </title>
    <meta name="description" content="{% block description %}Starter project with Express Nunjucks TailwindCSS{% endblock %}">
    <meta
    name="viewport" content="width=device-width, initial-scale=1"> {# <link rel="manifest" href="site.webmanifest"> #}
    <link rel="apple-touch-icon" href="/icon.png">
    <meta name="theme-color" content="#fafafa">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css">
    <link rel="stylesheet" href="/app.css">
  </head>
  <body class="h-full">
    <div class="min-h-full">
      {% include "partials/nav.njk" %}
      <header class="bg-white shadow">
        <div class="mx-auto max-w-7xl py-6 px-4 sm:px-6 lg:px-8">
          <h1 class="text-3xl font-bold tracking-tight text-gray-900">Dashboard</h1>
        </div>
      </header>
      <main>
        <div
          class="mx-auto max-w-7xl py-6 sm:px-6 lg:px-8">
          <!-- Replace with your content -->
          <div class="px-4 py-6 sm:px-0"> {% block content %}{% endblock %}
            </div>
            <!-- /End replace -->
          </div>
        </main>
      </div>
      <!-- [if IE]> <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a
      href="//browsehappy.com/" rel="nofollow noreferer">upgrade your browser</a> to improve your experience and security.</p> <![endif]
      -->
      <script src="https://cdnjs.cloudflare.com/ajax/libs/alpinejs/3.9.5/cdn.js"></script>
    </body>
  </html>