{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "downlevelIteration": true,
    "baseUrl": "./",
    "paths": { "src": ["./src"], "test": ["./test"] },
    "outDir": "./dist",
    "tsBuildInfoFile": "./tmp/tsconfig.tsbuildinfo",
    "esModuleInterop": true,
    "importHelpers": true
  },
  "include": ["./src/**/*", "./src/**/*.json", "./test/**/*"],
  "references": [{ "path": "../hexo-post-parser" }, { "path": "../git-command-helper" }]
}
