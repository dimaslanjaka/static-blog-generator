{
  "name": "sbg-utility",
  "version": "1.1.6",
  "description": "static-blog-generator utility",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "!**/release*/**"
  ],
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "update:ncu": "npx npm-check-updates -u -x async,progress,cheerio,jsdom,deepmerge-ts --enginesNode",
    "update": "run-s \"update:**\"",
    "test": "yarn run clean && jest --runInBand --detectOpenHandles",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "build": "gulp build && rollup -c -m",
    "build-pack": "yarn run build && yarn run pack",
    "clean": "gulp --series clean build",
    "pack": "node packer.js --yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/static-blog-generator.git"
  },
  "keywords": [
    "static-blog-generator",
    "utility"
  ],
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com> (https://www.webmanajemen.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/static-blog-generator/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/static-blog-generator#readme",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "async": "^3.2.4",
    "axios": "^1.6.7",
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.12",
    "cross-spawn": "*",
    "debug": "^4.3.4",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "gulp": "^4.0.2",
    "hexo": "^7.1.1",
    "hexo-log": "^4.1.0",
    "hexo-util": "^3.2.0",
    "jsdom": "^21.1.2",
    "jsonc-parser": "^3.2.1",
    "micromatch": "^4.0.5",
    "minimatch": "^9.0.3",
    "node-libcurl": "^3.0.0",
    "nunjucks": "^3.2.4",
    "plugin-error": "^2.0.1",
    "progress": "^2.0.3",
    "request": "^2.88.0",
    "safelinkify": "^1.2.2",
    "slugify": "^1.6.6",
    "through2": "^4.0.2",
    "tslib": "^2.6.2",
    "upath": "^2.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@jest/environment": "^29.5.0",
    "@jest/expect": "^29.5.0",
    "@jest/globals": "^29.5.0",
    "@jest/types": "^29.5.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/async": "^3.2.24",
    "@types/bluebird": "^3.5.42",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4",
    "@types/gulp-cache": "^0.4.9",
    "@types/jsdom": "21",
    "@types/micromatch": "^4.0.6",
    "@types/node": "^20.11.19",
    "@types/nunjucks": "^3.2.6",
    "@types/progress": "^2.0.7",
    "@types/request": "^2.48.12",
    "@types/through2": "https://github.com/dimaslanjaka/nodejs-package-types/tarball/through2",
    "git-command-helper": "^2.0.2",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "jest-mock": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.29.4",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  },
  "resolutions": {
    "hexo": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo.tgz",
    "hexo-asset-link": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo-asset-link.tgz",
    "hexo-cli": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo-cli.tgz",
    "hexo-front-matter": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo-front-matter.tgz",
    "hexo-log": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo-log.tgz",
