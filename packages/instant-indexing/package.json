{
  "name": "instant-indexing",
  "version": "1.0.4",
  "packageManager": "yarn@3.2.0",
  "description": "Easy Google Instant Indexing API. Force google to index your articles.",
  "main": "dist/index.js",
  "files": [
    "dist",
    "readme.md",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "start": "node -r ts-node/register -r dotenv/config start.ts",
    "build": "tsc --build tsconfig.build.json",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update": "run-s update:**",
    "pack": "node packer.js --yarn",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "update:ncu": "npx npm-check-updates -u",
    "test": "jest --setupFiles dotenv/config --runInBand"
  },
  "keywords": [
    "googleapis",
    "instant-indexing",
    "jwt",
    "google"
  ],
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "Dimas Lanjaka",
    "url": "https://www.webmanajemen.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/dimaslanjaka/static-blog-generator/tree/master/packages/instant-indexing#readme",
  "repository": {
    "directory": "packages/instant-indexing",
    "type": "git",
    "url": "https://github.com/dimaslanjaka/static-blog-generator"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.1",
    "@googleapis/docs": "^2.0.1",
    "@googleapis/indexing": "^1.0.0",
    "@googleapis/searchconsole": "^1.0.0",
    "axios": "^1.4.0",
    "fs-extra": "^11.1.1",
    "google-auth-library": "^8.8.0",
    "googleapis": "^118.0.0",
    "open": "^9.1.0",
    "request": "^2.88.2",
    "sbg-utility": "workspace:^",
    "server-destroy": "^1.0.1",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.1.1",
    "@types/server-destroy": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "jsonc-parser": "^3.2.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "types": "./dist/index.d.ts",
  "directories": {
    "test": "test"
  }
}
