{
  "name": "sbg-api",
  "version": "1.1.1",
  "description": "Pre-processing all source posts before rendering from hexo. Useful for low-end devices to prevent using large number of hexo plugins.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "readme.md",
    "!**/*.tsbuildinfo",
    "!**/release*/**"
  ],
  "scripts": {
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update:ncu": "npx npm-check-updates --enginesNode -u",
    "update": "run-s \"update:**\"",
    "test": "jest --runInBand --detectOpenHandles",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "build": "gulp build",
    "build-pack": "yarn run build && yarn run pack",
    "clean": "rm -rf dist tmp && yarn build",
    "pack": "node packer.js --yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/static-blog-generator.git"
  },
  "keywords": [
    "static-blog-generator"
  ],
  "author": "Dimas Lanjaka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/static-blog-generator/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/static-blog-generator#readme",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "bluebird": "^3.7.2",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/bf2d8bb/release/cross-spawn.tgz",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.2.0",
    "git-command-helper": "^2.0.2",
    "glob": "^10.3.10",
    "gulp": "^4.0.2",
    "hexo": "^7.1.1",
    "hexo-asset-link": "^2.2.3",
    "hexo-cli": "^4.3.1",
    "hexo-front-matter": "^4.2.1",
    "hexo-is": "^1.0.5",
    "hexo-log": "^4.1.0",
    "hexo-post-parser": "^1.2.14",
    "hexo-util": "^3.2.0",
    "micromatch": "^4.0.5",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "nunjucks": "^3.2.4",
    "safelinkify": "^1.2.2",
    "sbg-utility": "*",
    "true-case-path": "^2.2.1",
    "upath": "^2.0.1",
    "warehouse": "^5.0.1",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@jest/environment": "^29.5.0",
    "@jest/expect": "^29.5.0",
    "@jest/globals": "^29.5.0",
    "@jest/types": "^29.5.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tsconfig/node14": "^14.1.0",
    "@types/async": "^3.2.24",
    "@types/bluebird": "^3.5.42",
    "@types/debug": "^4.1.12",
    "@types/eslint": "^8",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/gulp-cache": "^0.4.9",
    "@types/hexo": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo.tgz",
    "@types/jsdom": "21",
    "@types/micromatch": "^4.0.6",
    "@types/node": "^20.11.19",
    "@types/nunjucks": "^3.2.6",
    "@types/progress": "^2.0.7",
    "@types/request": "^2.48.12",
    "@types/through2": "https://github.com/dimaslanjaka/nodejs-package-types/tarball/through2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "git-command-helper": "^2.0.2",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "jest-mock": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rollup": "^3.29.4",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16",
    "hexo": ">=6"
  },
  "resolutions": {
    "hexo": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo.tgz",
    "hexo-asset-link": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo-asset-link.tgz",
    "hexo-cli": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo-cli.tgz",
    "hexo-front-matter": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo-front-matter.tgz",
    "hexo-log": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo-log.tgz",
    "hexo-util": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/hexo-util.tgz",
    "warehouse": "https://github.com/dimaslanjaka/hexo/raw/6c026b7/releases/warehouse.tgz",
    "hexo-post-parser": "https://github.com/dimaslanjaka/hexo-post-parser/raw/8c9f16e/release/hexo-post-parser.tgz"
  },
  "packageManager": "yarn@3.8.0"
}
