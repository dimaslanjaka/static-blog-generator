{
  "name": "sbg-api",
  "version": "2.0.0",
  "description": "Pre-processing all source posts before rendering from hexo. Useful for low-end devices to prevent using large number of hexo plugins.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      },
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      }
    },
    "./*": {
      "import": {
        "default": "./dist/src/**/*.mjs",
        "types": "./dist/src/**/*.d.mts"
      },
      "require": {
        "default": "./dist/src/**/*.cjs",
        "types": "./dist/src/**/*.d.ts"
      }
    }
  },
  "files": [
    "dist/",
    "!**/*.tsbuildinfo"
  ],
  "workspaces": [
    "test/fixtures",
    "packages/*"
  ],
  "scripts": {
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.cjs",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.cjs",
    "update:ncu": "npx npm-check-updates -u",
    "update": "run-s \"update:**\"",
    "test": "cross-env-shell NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prebuild": "node src/index.builder.mjs",
    "build": "gulp build",
    "build-pack": "yarn run build && yarn run pack",
    "clean": "rm -rf dist tmp && yarn build",
    "prepack": "node package-switch.cjs production",
    "pack": "node packer.cjs --yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/static-blog-generator.git"
  },
  "keywords": [
    "static-blog-generator",
    "hexo",
    "markdown",
    "markdown-it",
    "jekyll"
  ],
  "author": "Dimas Lanjaka",
  "license": "MIT",
