{
  "name": "sbg-api",
  "version": "3.0.0",
  "description": "Pre-processing all source posts before rendering from hexo. Useful for low-end devices to prevent using large number of hexo plugins.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update": "npm run update:postinstall && npx npm-check-updates -u && npm install && node postinstall.js",
    "test": "jest --runInBand",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "build": "gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/static-blog-generator.git"
  },
  "keywords": [
    "static-blog-generator"
  ],
  "author": "Dimas Lanjaka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/static-blog-generator/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/static-blog-generator#readme",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^11.1.0",
    "git-command-helper": "https://github.com/dimaslanjaka/git-command-helper/raw/master/release/git-command-helper.tgz",
    "glob": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-dom": "https://github.com/dimaslanjaka/static-blog-generator-hexo/raw/master/packages/gulp-dom/release/gulp-dom.tgz",
    "hexo": "^6.3.0",
    "hexo-is": "^1.0.4",
    "hexo-post-parser": "https://github.com/dimaslanjaka/hexo-post-parser/raw/master/release/hexo-post-parser.tgz",
    "hexo-util": "^3.0.1",
    "micromatch": "^4.0.5",
    "moment-timezone": "^0.5.40",
    "nodejs-package-types": "https://github.com/dimaslanjaka/nodejs-package-types/raw/main/release/nodejs-package-types.tgz",
    "nunjucks": "^3.2.3",
    "persistent-cache": "https://github.com/dimaslanjaka/persistent-cache/raw/improve2/release/persistent-cache.tgz",
    "safelinkify": "https://github.com/dimaslanjaka/safelink/raw/master/release/safelinkify.tgz",
    "sbg-utility": "file:../sbg-utility",
    "sitemap-crawler": "https://github.com/dimaslanjaka/static-blog-generator-hexo/raw/master/packages/sitemap-crawler/release/sitemap-crawler.tgz",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2"
  }
}
