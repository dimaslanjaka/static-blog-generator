    "git-command-helper": "^2.0.2",
    "glob": "^11.0.0",
    "gulp": "^5.0.0",
    "hexo": "^7.3.0",
    "hexo-asset-link": "^2.2.3",
    "hexo-cli": "^4.3.2",
    "hexo-front-matter": "^4.2.1",
    "hexo-is": "^1.0.5",
    "hexo-log": "^4.1.0",
    "hexo-post-parser": "^1.2.14",
    "hexo-util": "^3.3.0",
    "marked": "^14.1.3",
    "micromatch": "^4.0.8",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "nunjucks": "^3.2.4",
    "p-limit": "^6.1.0",
    "safelinkify": "^1.2.2",
